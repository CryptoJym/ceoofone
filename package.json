{
  "name": "ceo-of-one",
  "version": "1.0.0",
  "description": "CEO of One - AI-powered chief of staff for solo CEOs",
  "scripts": {
    "orchestrate": "node scripts/agent-orchestrator.js",
    "background": "node scripts/run-agent.js",
    "agent": "node scripts/run-agent.js",
    "dev": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "dev:full": "concurrently \"npm run dev:backend\" \"npm run dev\"",
    "build": "npm run build:backend && cd frontend && npm run build",
    "build:backend": "cd backend && npm run build",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "test": "jest",
    "test:backend": "cd backend && npm run test",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:backend": "cd backend && npm run lint"
  },
  "keywords": [
    "ceo",
    "solo-entrepreneur",
    "ai-assistant",
    "productivity",
    "utlyze"
  ],
  "author": "Utlyze",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "typescript": "^5.0.0",
    "concurrently": "^8.2.2"
  }
}